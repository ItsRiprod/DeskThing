{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "src/shared/types/*.ts"
, "src/shared/types/ipc/ipcTypes.ts", "src/shared/types/ipc/ipcPlatform.ts", "src/main/stores/githubStore.ts"    ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@shared/*": ["src/shared/*"]
    }
  }
}
