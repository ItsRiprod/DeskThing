{
  "name": "deskthing",
  "version": "0.10.3",
  "description": "A DeskThing server UI to interface with the DeskThing car thing app",
  "main": "./out/main/index.js",
  "author": "Riprod",
  "type": "module",
  "homepage": "https://github.com/ItsRiprod/DeskThing",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "debug": "electron-vite dev --inspect=8893",
    "build": "npm run typecheck && electron-vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itsriprod/DeskThing.git"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/auto-launch": "^5.0.5",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.14",
    "adm-zip": "^0.5.16",
    "auto-launch": "^5.0.6",
    "cors": "^2.8.5",
    "cross-unzip": "^0.2.1",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "fast-average-color-node": "^3.1.0",
    "lottie-react": "^2.4.1",
    "react-qr-code": "^2.0.15",
    "react-rewards": "^2.1.0",
    "react-router-dom": "^7.1.5",
    "react-select": "^5.10.0",
    "react-tag-input": "^6.10.3",
    "uuid": "^11.0.5",
    "ws": "^8.18.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^6.0.11"
  }
}
